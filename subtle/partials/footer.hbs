<footer class="site-footer">
	<div class="inner">
		<div class="offsite-links">
			{{#if @blog.twitter}}
			<a href="{{twitter_url}}" class="circle" target="_blank"><i class="icon-twitter" aria-hidden="true"></i><span class="screen-reader-text">Twitter</span></a>
			{{/if}}
			{{#if @blog.facebook}}
			<a href="{{facebook_url}}" class="circle" target="_blank"><i class="icon-facebook" aria-hidden="true"></i><span class="screen-reader-text">Facebook</span></a>
			{{/if}}
			<a href="https://plus.google.com/{{! Add Google+ handle }}" class="circle" target="_blank"><i class="icon-google" aria-hidden="true"></i><span class="screen-reader-text">Google+</span></a>
			<a href="https://www.linkedin.com/in/{{! Add Linkedin handle }}" class="circle" target="_blank"><i class="icon-linkedin" aria-hidden="true"></i><span class="screen-reader-text">Linkedin</span></a>
			<a href="https://github.com/{{! Add GitHub handle }}" class="circle" target="_blank"><i class="icon-github" aria-hidden="true"></i><span class="screen-reader-text">GitHub</span></a>
			<a href="https://codepen.io/{{! Add CodePen handle }}" class="circle" target="_blank"><i class="icon-codepen" aria-hidden="true"></i><span class="screen-reader-text">CodePen</span></a>
			<a href="https://dribbble.com/{{! Add Dribbble handle }}" class="circle" target="_blank"><i class="icon-dribbble" aria-hidden="true"></i><span class="screen-reader-text">Dribbble</span></a>
			<a href="https://www.behance.net/{{! Add Behance handle }}" class="circle" target="_blank"><i class="icon-behance" aria-hidden="true"></i><span class="screen-reader-text">Behance</span></a>
			<a href="https://medium.com/{{! Add Medium handle }}" class="circle" target="_blank"><i class="icon-medium" aria-hidden="true"></i><span class="screen-reader-text">Medium</span></a>
			<a href="https://www.instagram.com/{{! Add Instagram handle }}" class="circle" target="_blank"><i class="icon-instagram" aria-hidden="true"></i><span class="screen-reader-text">Instagram</span></a>
			<a href="https://www.pinterest.com/{{! Add Pinterest handle }}" class="circle" target="_blank"><i class="icon-pinterest" aria-hidden="true"></i><span class="screen-reader-text">Pinterest</span></a>
			<a href="https://www.vimeo.com/{{! Add Vimeo handle }}" class="circle" target="_blank"><i class="icon-vimeo" aria-hidden="true"></i><span class="screen-reader-text">Vimeo</span></a>
			<a href="https://www.youtube.com/user/{{! Add YouTube handle }}" class="circle" target="_blank"><i class="icon-youtube" aria-hidden="true"></i><span class="screen-reader-text">YouTube</span></a>
			<a href="{{@blog.url}}/rss/" class="circle" target="_blank"><i class="icon-feed" aria-hidden="true"></i><span class="screen-reader-text">RSS</span></a>
		</div><!-- .offsite-links -->
		<div class="site-info">
			&copy; <a href="/">{{@blog.title}}</a> all rights reserved.<br />
			Powered by <a target="_blank" href="https://ghost.org/">Ghost</a>. Subtle theme by <a target="_blank" href="https://justgoodthemes.com/">JustGoodThemes</a>
		</div><!-- .site-info -->
		<a href="#page" class="arrow-up" title="Back to Top"><span class="screen-reader-text">Back to the top</span></a>
	</div><!-- .inner -->
</footer><!-- .site-footer -->
